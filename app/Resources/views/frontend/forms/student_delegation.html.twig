{% block form %}
    {{ form_errors(form) }}
    <div class="row">
        <div class="col-md-6">
            {{ form_row(form.name) }}
            {{ form_row(form.address) }}
            {{ form_row(form.city) }}
            {{ form_row(form.province) }}
            {{ form_row(form.postcode) }}
            {{ form_row(form.cif) }}
        </div>
        <div class="col-md-6">
            {{ form_row(form.phone) }}
            {{ form_row(form.fax) }}
            {{ form_row(form.email) }}
            {{ form_row(form.web) }}
            {{ form_row(form.facebook) }}
            {{ form_row(form.twitter) }}
        </div><!-- /.col -->
    </div><!-- /.row -->
    {{ form_rest(form) }}
{% endblock %}